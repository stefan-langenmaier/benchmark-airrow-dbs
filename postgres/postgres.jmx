        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Geo Request" enabled="true">
          <stringProp name="dataSource">search</stringProp>
          <stringProp name="query">--SELECT * FROM bench ORDER BY location &lt;-&gt; ST_GeomFromText(&apos;POINT(${latitude} ${longitude})&apos;, 4326) LIMIT 10000;
WITH index_query AS (
  SELECT ST_Distance(location, ST_GeomFromText(&apos;POINT(${latitude} ${longitude})&apos;, 4326)) as d, status
	FROM bench ORDER BY location &lt;-&gt; ST_GeomFromText(&apos;POINT(${latitude} ${longitude})&apos;, 4326) LIMIT 1000)
  SELECT *
	FROM index_query
  ORDER BY d*jaro(status, &apos;EMOJI&apos;)
  LIMIT 1;</stringProp>
          <stringProp name="queryArguments"></stringProp>
          <stringProp name="queryArgumentsTypes"></stringProp>
